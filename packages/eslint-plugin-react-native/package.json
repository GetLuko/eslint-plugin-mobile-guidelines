{
  "name": "@getluko/eslint-plugin-react-native",
  "description": "GetLuko React Native ESLint Plugin created by the App Core guild",
  "version": "0.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetLuko/eslint-plugin-react-native"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "plugin",
    "getluko",
    "javascript",
    "styleguide",
    "typescript",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "contributors": [
    {
      "name": "Anthony Lasserre",
      "url": "https://github.com/anthlasserre"
    },
    {
      "name": "Mickael Dumand",
      "url": "https://github.com/kimak"
    }
  ],
  "bugs": {
    "url": "https://github.com/GetLuko/eslint-plugin-react-native/issues"
  },
  "homepage": "https://github.com/GetLuko/eslint-plugin-react-native",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/parser": "5.14.0",
    "@typescript-eslint/types": "5.13.0",
    "@typescript-eslint/utils": "5.13.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "eslint": "^8.18.0",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "peerDependencies": {
    "eslint": "^6 || ^7.2.0"
  },
  "jest": {
    "testRegex": ".*\\.test\\.ts$",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "engines": {
    "node": "^10.12.0 || ^12.22.0 || ^14.17.0 || >=16.0.0"
  }
}
