{
  "name": "@getluko/eslint-plugin-mobile-guidelines",
  "author": "luko",
  "version": "0.0.1",
  "description": "Custom ESLint Rules & Config created by mobile team",
  "main": "dist/",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "postinstall": "npm run build",
    "postbuild": "rimraf dist/**/__tests__"
  },
  "dependencies": {
    "@typescript-eslint/parser": "5.14.0",
    "@typescript-eslint/types": "5.13.0",
    "@typescript-eslint/utils": "5.13.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "eslint": "^8.18.0",
    "jest": "27.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "jest": {
    "testRegex": ".*\\.test\\.ts$",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "peerDependencies": {
    "eslint": ">=2.0.0"
  }
}
